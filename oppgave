public class Oppgaver {

	public static void main(String[] args) {
		print(fyll(10));
	}
	
	//oppgave a)
	public static int[] fyll(int y) {
		int[] fyll;
		fyll = new int[y];
		for (int i = 0; i < y; i++) {
			fyll[i] = (int) (Math.random() * y);
		}
		return fyll;
	}

	//oppgave b)
	public static void print(int[] fyll) {
		for (int i = 0; i < fyll.length; i++) { 		// Forelesning 10, side 35
			System.out.printf("%3d", fyll[i]);
		}
	}
	
	//oppgave c)
	public static void sjekk(int[] fyll, int x){ 		// 
						
						// Her mÃ¥ du ha en int med verdi 0
						// forelesning 10, side 35
						// if "i" er likt som x, pluss 1.
						// }
						// return inten
	}
}
